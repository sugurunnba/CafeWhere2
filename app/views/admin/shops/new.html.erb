<div class="container">
  <div class="row">
    <div class="col-md-10 col-md-offset-1">
      <table border="1" style="border-collapse: collapse">
        <tr>
          <h3 class="text-center">登録申請カフェ一覧</h3>
        </tr>
<br>
        <% @shops.each do |shop| %>
          <% if shop.is_active == false %>
          <tbody>
            <tr>
              <td><%= shop.id %></td>
              <td>
                <%= link_to edit_admin_shop_path(shop) do %>
                  <%= shop.name %>
                <% end %>
              </td>
            </tr>
          </tbody>
          <% end %>
        <% end %>
      </table>
    </div>
  </div>
</div>
