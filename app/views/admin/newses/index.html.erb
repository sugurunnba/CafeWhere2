<h3>お知らせ一覧</h3>

<%= link_to "お知らせ登録", new_admin_newse_path %>

<table class="table">
  <thead>
    <tr>
      <td>お知らせID</td>
      <td>お知らせタイトル</td>
    </tr>
  </thead>
  <% @newses.each do |news| %>
  <tbody>
    <tr>
      <td><%= news.id %></td>
      <td>
        <%= link_to admin_newses_path(news) do %>
          <%= news.title %>
        <% end %>
      </td>
    </tr>
  </tbody>
  <% end %>
</table>