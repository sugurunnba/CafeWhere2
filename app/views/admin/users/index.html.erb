<div class="container">
  <div class="row">
    <div class="col-md-10 col-md-offset-1">
      <table border="1" style="border-collapse: collapse">
        <thead>
          <tr>
            <td>ユーザーID</td>
            <td>ユーザー名</td>
          </tr>
        </thead>
        <% @users.each do |user| %>
        <tbody>
          <tr>
            <td><%= user.id %></td>
            <td>
              <%= link_to admin_user_path(user) do %>
                <%= user.name %>
              <% end %>
            </td>
          </tr>
        </tbody>
        <% end %>
      </table>
    </div>
  </div>
</div>

