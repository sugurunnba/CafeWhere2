<h3>お問い合わせフォーム</h3>

<%= form_with model: @contact, url: user_contacts_path do |f| %>
<table>
  <tr>
    <td><%= f.label :お名前 %></td>
  </tr>
  <tr>
    <td><%= f.text_field :name %></td>
  </tr>
   <tr>
    <td><%= f.label :お問い合わせ内要 %></td>
  </tr>
   <tr>
    <td><%= f.text_area :message %></td>
  </tr>
   <tr>
    <td><%= f.submit "送信" %></td>
  </tr>
</table>
<% end %>